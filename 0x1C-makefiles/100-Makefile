CC = gcc
SRC = main.c holberton.c
NAME = holberton
OBJ = $(SRC:.c=.o)
RM= rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic
all: m.h $(OBJ)
	$(CC) -Wall -Werror -Wextra -pedantic $(OBJ) -o $(NAME)
.PHONY: clean oclean fclean re
clean:
	$(RM) *~ *.swp $(NAME)
oclean:
	$(RM) $(OBJ)
fclean: oclean clean
re: oclean all
